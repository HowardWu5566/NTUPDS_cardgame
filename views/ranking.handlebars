{{> background}}
{{> header}}

<main id="ranking" class="flex-container">
  {{> loading}}
  <table>
    <thead>
      <tr>
        <th>排行</th>
        <th>名號</th>
        <th>得分</th>
      </tr>
    </thead>
    <tbody>
      {{#each masterArr}}
      <tr>
        <td>{{this.rank}}</td>
        <td>{{this.name}}</td>
        <td>{{this.score}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  <button class="btn" onclick="getPage('/')">返回</button>
</main>

{{> footer}}